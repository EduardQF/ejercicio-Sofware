Proceso promedio
	Escribir "ingrese tiempo demorados en minutos";
	Escribir "dia lunes ";
	leer lu;
	Escribir "dia martes";
	leer ma;
	Escribir "dia miercoles";
	leer mi;
	Escribir "dia jueves";
	leer ju;
	Escribir "dia viernes";
	leer vi;
	Escribir "dia sabado";
	leer sa;
	prom<- (lu + ma + mi + ju + vi + sa)/6;
	Escribir "el tiempo promedio recorrido es:", prom;
FinProceso

